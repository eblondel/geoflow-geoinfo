{
  "id": "grsf",
  "profile": {
	"project": "GRSF",
	"organization": "GRSF",
	"logos": [],
	"environment": {
  		"file": "envs/geoinfo.env",
  		"hide_env_vars": ["USER", "PASSWORD", "TOKEN"]
  	},
    "mode": "entity",
    "options": {
      "skipEnrichWithDataSubjects": true
    }
  },

  "metadata": {
    "entities": [{
      "handler": "gsheet",
      "source": "https://docs.google.com/spreadsheets/d/15Z-9NYNSmfeB5dCBDuma7ZMrWYmCWsIUz-VXQMWbk5s/edit?gid=1326213774"
    }],
	"contacts" : [{
      "handler": "gsheet",
      "source": "https://docs.google.com/spreadsheets/d/15Z-9NYNSmfeB5dCBDuma7ZMrWYmCWsIUz-VXQMWbk5s/edit?gid=504815837"
    }],
	"dictionary": [{
	  "handler": "gsheet",
	  "source": "https://docs.google.com/spreadsheets/d/15Z-9NYNSmfeB5dCBDuma7ZMrWYmCWsIUz-VXQMWbk5s/edit?gid=2060976687"
	}]
  },
  "software": [
	{
		"id": "fao-nfi-geonetwork",
		"type": "output",
		"software_type": "geonetwork",
		"parameters": {
			"url": "{{GEONETWORK_URL}}",
			"user": "{{GEONETWORK_USER}}",
			"pwd": "{{GEONETWORK_PASSWORD}}",
			"version": "{{GEONETWORK_VERSION}}",
			"logger": "INFO"
		}
	},
	{
		"id": "fao-nfi-geoserver",
		"type": "output",
		"software_type": "geoserver",
		"parameters": {
			"url": "{{GEOSERVER_URL}}",
			"user": "{{GEOSERVER_USER}}",
			"pwd": "{{GEOSERVER_PASSWORD}}",
			"logger": "INFO"
		},
		"properties" : {
			"workspace": "grsf"
		}
	},
	{
		"id": "googledrive",
		"type": "input",
		"software_type": "googledrive",
		"parameters": {
			"email": "{{GDRIVE_FISHERIESATLAS_EMAIL}}",
			"token": "{{GDRIVE_FISHERIESATLAS_TOKEN}}"
		},
		"properties" : {}
	},
	{
		"id": "zenodo",
	  "type": "output",
	  "software_type": "zenodo",
	  "parameters": {
  		"url": "https://zenodo.org/api",
  		"token": "{{ ZENODO_TOKEN }}",
  		"logger": "DEBUG"
	  }
	}
  ],
  "actions": [
  {
	  "id": "zen4R-deposit-record",
	  "run": true,
	  "options": {
		  "zipEachDataFile": false
	  }
	},
  {
		"id": "grsf_areas_changelog",
		"script": "../actions/grsf/grsf_areas_changelog.R",
		"run": true
  },
  {
		"id": "grsf_areas_vocabulary_files",
		"script": "../actions/grsf/grsf_areas_vocabulary_files.R",
		"run": true
  },
  {
    "id": "geometa-create-iso-19115",
    "run": true,
	  "options": {
		  "logo": true,
		  "doi": true,
		  "include_object_identification_ids": true
	  }
  },
	{
    "id": "geometa-create-iso-19110",
    "run": true,
	  "options": {
		  "exclude_values_for_attributes" : ["ID","namespace","system_owner_name","code_system_name","differentiating_code_system","differentiating_code_system_description","area_name","source_link","use_limitation"]
	  }
  },
	{
	  "id": "geonapi-publish-iso-19139",
	  "run": true
	},
	{
	  "id": "geosapi-publish-ogc-services",
	  "run": true,
	  "options": {
		  "createStore": true
	  }
	}
  ]
}
