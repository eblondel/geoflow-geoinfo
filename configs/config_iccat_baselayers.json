{
	"profile": {
		"id": "vme-workflow",
		"mode": "entity",
		"logos": [
    	"https://www.iccat.int/img/logoICCAT.png"
  	],
		"environment": {
			"file": "envs/geoinfo.env",
			"hide_env_vars": ["USER", "PASSWORD", "TOKEN"]
		}
	},
	 "metadata": {
	    "entities": [{
  		  "handler": "gsheet",
  		  "source": "https://docs.google.com/spreadsheets/d/11oXcDFUixXBuXlDxJqL8RlNV9s6W-1nyBLBtEMtgbpQ/edit?gid=430029225#gid=430029225"
	    }],
	    "contacts" : [{
        "handler": "gsheet",
        "source": "https://docs.google.com/spreadsheets/d/11oXcDFUixXBuXlDxJqL8RlNV9s6W-1nyBLBtEMtgbpQ/edit?gid=1005396552#gid=1005396552"
      }],
	    "dictionary": [{
	      "handler": "gsheet",
	      "source": "https://docs.google.com/spreadsheets/d/11oXcDFUixXBuXlDxJqL8RlNV9s6W-1nyBLBtEMtgbpQ/edit?gid=227860508#gid=227860508"
	    }]
	},
	"software": [
	{
		"id": "nfi_wfs",
		"type": "input",
		"software_type": "wfs",
		"parameters": {
			"url": "https://www.fao.org/fishery/geoserver/wfs",
			"user": "{{GEOSERVER_USER}}",
			"pwd": "{{GEOSERVER_PASSWORD}}",
			"serviceVersion": "1.0.0",
			"logger": "DEBUG"
		}
	},
	{
		"id": "nfi-geonetwork",
		"type": "output",
		"software_type": "geonetwork",
		"parameters": {
			"url": "{{GEONETWORK_URL}}",
			"user": "{{GEONETWORK_USER}}",
			"pwd": "{{GEONETWORK_PASSWORD}}",
			"version": "{{GEONETWORK_VERSION}}",
			"logger": "INFO"
		}
	},
	{
		"id": "nfi-geoserver",
		"type": "output",
		"software_type": "geoserver",
		"parameters": {
			"url": "{{GEOSERVER_URL}}",
			"user": "{{GEOSERVER_USER}}",
			"pwd": "{{GEOSERVER_PASSWORD}}",
			"logger": "DEBUG"
		},
		"properties": {
			"workspace": "iccat"
		}
	}
	],
	"actions": [
	  {
  	  "id": "process_master",
  	  "script": "../actions/core/process_master.R",
  	  "run":true
	  },
	  {
    "id": "geometa-create-iso-19110",
    "run": true,
	  "options": {
		  "exclude_values_for_attributes" : ["fid","label_en","label_fr","label_es","stock"]
	   }
    },
	  {
      "id": "geometa-create-iso-19115",
      "run": true,
  	  "options": {
  	      "logo": true,
  	      "include_object_identification_ids": true
  	  }
	  },
	  {
  	  "id": "geonapi-publish-iso-19139",
  	  "run":true
	  },
	  {
  	  "id": "geosapi-publish-ogc-services",
  	  "run":true,
  	  "options": {
  		  "createWorkspace": true,
  		  "createStore": true,
  		  "overwrite": true,
  		  "enrich_with_relation_wfs_download_links": true,
  		  "describe_wms_relation_with_category": true,
          "describe_wms_relation_with_ogc_service_description" : false
  	    }
	  }
	  
	]
}
